    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string": "^0.2.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
